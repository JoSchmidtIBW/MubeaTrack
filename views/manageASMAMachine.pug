extends base

block content
    main.main
      h1 Consists of sectors, components, details
      <br>
      <br>
      table#manageASMAMachineTable.manageASMAMachineTable.table
        thead
          tr
            th ID
            th Name
            th Description
            th Department
            th ASMAmachinery
            th Edit
        tbody
          each machine in machinery
            tr
              td= machine._id
              td= machine.name
              td= machine.description
              td= machine.department
              td
                  each sector in machine.sectorASMA
                    p= sector.name
              td
                a.edit-button(href=`/api/v1/createASMAmachine/${machine._id}`) Edit

      br
